{
  "name": "QuACS",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "./scripts/build_entry.sh -d",
    "build": "./scripts/build_entry.sh -b",
    "test:serve": "./scripts/build_entry.sh -d -t",
    "test:e2e": "vue-cli-service test:e2e --url http://localhost:8080/summer2021/",
    "test:e2e:headless": "vue-cli-service test:e2e --headless --url http://localhost:8080/summer2021/",
    "debug-build": "./scripts/build_entry.sh -d -b",
    "lint": "./scripts/lint.sh",
    "gh-pages-deploy": "sh scripts/gh-pages-deploy.sh",
    "postinstall": "cd .. && husky install site/.husky"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/vue-fontawesome": "3.0.8",
    "@trevoreyre/autocomplete-vue": "3.0.3",
    "@types/file-saver": "2.0.7",
    "axios": "1.9.0",
    "bootstrap": "5.3.6",
    "bootstrap-vue": "2.23.1",
    "file-saver": "2.0.5",
    "fuse.js": "7.1.0",
    "ics": "3.8.1",
    "package.json": "2.0.1",
    "register-service-worker": "1.7.2",
    "simple-web-worker": "1.2.0",
    "stylelint": "16.20.0",
    "stylelint-config-standard": "38.0.0",
    "tslib": "2.8.1",
    "vue": "3.5.16",
    "vue-async-computed": "4.0.1",
    "vue-axios": "3.5.2",
    "vue-class-component": "7.2.6",
    "vue-property-decorator": "9.1.2",
    "vue-router": "4.5.1",
    "vue-visjs": "0.6.0",
    "vuex": "4.1.0",
    "vuex-persistedstate": "4.1.0",
    "xss": "1.0.15",
    "yarn.lock": "0.0.1-security"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "6.0.4",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "@vue/cli-plugin-e2e-cypress": "5.0.8",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-plugin-pwa": "5.0.8",
    "@vue/cli-plugin-router": "5.0.8",
    "@vue/cli-plugin-typescript": "5.0.8",
    "@vue/cli-plugin-vuex": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.5.0",
    "eslint": "9.28.0",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-vue": "10.2.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.0",
    "prettier": "3.5.3",
    "sass": "1.89.2",
    "sass-loader": "16.0.5",
    "typescript": "5.8.3",
    "vue-template-compiler": "2.7.16",
    "vuex-module-decorators": "2.0.0",
    "workerize-loader": "2.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "ignorePatterns": [
      "src/quacs-rs/**"
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "no-console": "warn",
      "@typescript-eslint/ban-ts-comment": [
        "error",
        {
          "ts-ignore": "allow-with-description",
          "ts-expect-error": "allow-with-description"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx,json}": [
      "vue-cli-service lint --max-warnings 0"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  }
}
