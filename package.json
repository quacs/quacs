		"vuex-module-decorators": "^0.17.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended",
			"@vue/typescript/recommended",
			"@vue/prettier",
			"@vue/prettier/@typescript-eslint"
